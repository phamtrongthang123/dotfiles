{
    "python.jediEnabled": false,
    "window.titleBarStyle": "custom",
    "workbench.colorTheme": "One Dark Pro Vivid",
    "vim.useSystemClipboard": true,
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before":["<C-n>"],
            "commands": [
                ":nohl",
            ]
        },
        {
            "before": [
                "<C-l>"
            ],
            "after": [
               "<C-w>","l"
            ]
        },
        {
            "before": [
                "<C-h>"
            ],
            "after": [
               "<C-w>","h"
            ]
        },
        {
            "before": [
                "j"
            ],
            "after": [
                "g",
                "j"
            ]
        },
        {
            "before": [
                "k"
            ],
            "after": [
                "g",
                "k"
            ]
        },
        {
            "before": [
                "<leader>",
                "m"
            ],
            "commands": [
                "bookmarks.toggle"
            ]
        },
        {
            "before": [
                "<leader>",
                "b"
            ],
            "commands": [
                "bookmarks.list"
            ]
        },
        {
            "before": [
                "leader",
                "w"
            ],
            "commands": [
                "workbench.action.files.save",
            ]
        },
        {
            "before":[
                "q","a"
            ],
            "commands": [
                ":tabclose"
            ]
        }
    ],
    "editor.wordWrap": "on",
    "editor.lineNumbers": "relative",
    "search.showLineNumbers": true,
    "vim.leader": "<space>",
    "vim.handleKeys": {
        "<C-a>": false,
        "<C-f>": false
    },
    "vim.hlsearch": true,
    "vim.incsearch": true,
    "vim.visualstar": true,
    

}